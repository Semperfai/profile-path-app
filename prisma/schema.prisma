generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users {
  id         Int         @id @default(autoincrement())
  userId     String      @db.Uuid
  theme_id   Int?         
  email      String      @unique
  name       String
  bio        String?
  image      String?
  created_at String
}

model Links {
  id         Int         @id @default(autoincrement())
  userId     String      
  name       String
  url        String
  image      String 
  created_at DateTime?   @default(now()) @db.Timestamptz(6)
}


